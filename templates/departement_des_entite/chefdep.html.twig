<div class="modal fade" id="createmodel" tabindex="-1" role="dialog" aria-labelledby="createModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
           {{ form_start(form, {'action': url)}) }}
                   <div class="modal-header">
                    <h5 class="modal-title" id="createModalLabel"><i class="ti-marker-alt m-r-10"></i> ticket for:</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                    </button>
                   </div>
                   <div class="modal-body">
                        {{ form_row(form.identifiant, { 'label': 'Matricule ou Numero de declaration' }) }}
                        {{ form_row(form.nom, { 'label': 'Nom' }) }}
                        {{ form_row(form.prenom, { 'label': 'Prenom' }) }}
                        {{ form_row(form.email, { 'label': 'E-mail' }) }}
                        {{ form_row(form.tel, { 'label': 'Telephone' }) }}
                        {{ form_row(form.arrondissement, { 'label': 'Arrondissement' }) }}
                        {{ form_row(form.quartier, { 'label': 'Quartier' }) }}
                        {{ form_row(form.ville, { 'label': 'Ville' }) }}
                        {{ form_row(form.adresse, { 'label': 'Adresse' }) }}
                        {{ form_row(form.date_de_naissance, { 'label': 'Date de naissance' }) }}
                        {{ form_row(form.poste, { 'label': 'Poste' }) }}
                        {{ form_row(form.postulant, { 'label': 'Postulant' }) }}
                   <div class="modal-footer">
                   </div>
                        <button type="submit" class="btn btn-success"><i class="ti-save"></i> Enregistrer</button>
                   </div>
             {{ form_end(form) }}
        </div>    
    </div>
</div>